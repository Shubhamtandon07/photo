import requests

host = "https://genai-nexus.api.corpinter.net"

paths = [
    "/",                      # sometimes 200/302
    "/openai",                # sometimes 404/401
    "/openai/",               
    "/openai/deployments",    # often 401/404
    "/v1",                    
    "/v1/",                   
    "/apikey",                # your current
    "/apikey/",               
]

for p in paths:
    url = host + p
    try:
        r = requests.get(url, timeout=10)
        print(f"{p:22} -> {r.status_code}  {r.text[:60].replace('\\n',' ')}")
    except Exception as e:
        print(f"{p:22} -> ERROR {e}")
