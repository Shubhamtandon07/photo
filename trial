import requests

host = "https://genai-nexus.api.corpinter.net"

paths = [
    "/docs", "/swagger", "/swagger/", "/swagger/index.html",
    "/openapi.json", "/api-docs", "/redoc",
    "/health", "/healthz", "/status", "/version",
    "/api", "/api/", "/api/v1", "/api/v1/",
    "/nexus", "/nexus/", "/genai", "/genai/",
    "/proxy", "/proxy/", "/gateway", "/gateway/",
    "/openai-proxy", "/openai-proxy/",
    "/azure", "/azure/", "/azureopenai", "/azureopenai/",
    "/aoai", "/aoai/",
    "/chat", "/chat/", "/completions", "/completions/",
]

methods = ["GET", "HEAD", "OPTIONS"]

for p in paths:
    url = host + p
    for m in methods:
        try:
            r = requests.request(m, url, timeout=10, allow_redirects=True)
            code = r.status_code
            if code != 404:
                print(f"{m:7} {p:25} -> {code}  server={r.headers.get('server')}  len={len(r.text)}")
        except Exception as e:
            pass
